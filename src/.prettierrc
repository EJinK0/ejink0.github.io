{
    "trailingComma": "none",
    "tabWidth": 4,
    "bracketSpacing": false,
    "printWidth": 110,
    "singleQuote": true,
    "arrowParens": "always",
    "endOfLine": "auto",
    "plugins": ["@trivago/prettier-plugin-sort-imports"],
    "importOrder": ["^react", "./stackflow", "<THIRD_PARTY_MODULES>", "^~[./]", "^[./]"],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true
}